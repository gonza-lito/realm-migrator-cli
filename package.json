{
  "name": "realm-migrator-cli",
  "description": "data migration tool for realm",
  "version": "0.0.0",
  "author": "Gonzalo Romano",
  "bin": {
    "realm-migrator-cli": "./bin/run"
  },
  "bugs": "https://github.com/gonza-lito/realm-migrator-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "JSONStream": "^1.3.5",
    "event-stream": "^4.0.1",
    "lodash": "^4.17.15",
    "realm": "^3.6.3",
    "rimraf": "^3.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.7",
    "@types/event-stream": "^3.3.34",
    "@types/jsonstream": "^0.8.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.13",
    "@types/rimraf": "^2.0.3",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/gonza-lito/realm-migrator-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "realm-migrator-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "gonza-lito/realm-migrator-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
